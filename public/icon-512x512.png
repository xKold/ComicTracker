<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#a855f7;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#9333ea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#d946ef;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background rounded square -->
  <rect width="512" height="512" rx="100" fill="url(#gradient)"/>
  
  <!-- Book icon -->
  <g transform="translate(256, 256)">
    <!-- Open book -->
    <path d="M -100 -80 L -100 80 L -10 70 L -10 -70 Z" fill="#ffffff" opacity="0.9"/>
    <path d="M 100 -80 L 100 80 L 10 70 L 10 -70 Z" fill="#ffffff" opacity="0.9"/>
    
    <!-- Bookmark -->
    <path d="M -10 -70 L -10 20 L 0 10 L 10 20 L 10 -70 Z" fill="#faf5ff"/>
    
    <!-- Pages lines on left -->
    <line x1="-80" y1="-40" x2="-30" y2="-40" stroke="#a855f7" stroke-width="4" opacity="0.6"/>
    <line x1="-80" y1="-10" x2="-30" y2="-10" stroke="#a855f7" stroke-width="4" opacity="0.6"/>
    <line x1="-80" y1="20" x2="-30" y2="20" stroke="#a855f7" stroke-width="4" opacity="0.6"/>
    
    <!-- Pages lines on right -->
    <line x1="30" y1="-40" x2="80" y2="-40" stroke="#a855f7" stroke-width="4" opacity="0.6"/>
    <line x1="30" y1="-10" x2="80" y2="-10" stroke="#a855f7" stroke-width="4" opacity="0.6"/>
    <line x1="30" y1="20" x2="80" y2="20" stroke="#a855f7" stroke-width="4" opacity="0.6"/>
  </g>
</svg>